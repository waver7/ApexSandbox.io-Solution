public Boolean duplicateContacts(Contact c1, Contact c2) {
    // Check if the email address of both contacts are equal and not null
    if (c1.Email != null && c2.Email != null && c1.Email == c2.Email) {
        return true;
    }
    
    // Check if the phone number of both contacts are equal and not null
    else if (c1.Phone != null && c2.Phone != null && c1.Phone == c2.Phone) {
        return true;
    }
    
    else{// If no duplicates are found, return false
    return false;
    }

}