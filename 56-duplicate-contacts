public Boolean duplicateContacts(Contact c1, Contact c2) {
    Contact c1 = new Contact();
    Contact c2 = new Contact();
    if(c1.Phone = null || c2.Phone = null c1.Email = null c2.Email = null ){
        return false;
    }
    else if (c1.Phone == c2.Phone || c1.Email = c2.Email) {
        return false;
    }
    else {
        return true;
    }
}