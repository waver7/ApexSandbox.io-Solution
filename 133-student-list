public void insertStudents(List<String> studentNames, List<String> studentEmails) {

    <List>apxio__Student__c newStudent = new apxio__Student__c();

    for (Integer i = 0; i < studentNames.size(); i++){
        newst.Name = studentNames[i];
        newst.apxio__Email__c = studentEmails[i];
        insert newst;

    }
    
    insert newStudent;



}